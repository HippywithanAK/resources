#---------------------
#Git Command Aliases
#---------------------

#print git log
alias gl='git log'

# initiate repo
alias gi='git init'

# clone repo
alias gcl='git clone'

# add to staging
alias ga='git add'

# commit changes
alias gc='git commit'

# Add remote 
alias grau='git remote add upstream'

# push changes to remote (if available)
alias gps='git push' 

# pull from origin
alias gpl='git pull'

# merge fork or branch
alias gm='git merge'

# check branch
alias gb='git branch'

# change to branch
alias gch='git checkout'

# shutdown shortcut
alias sdn='shutdown now'

# Restart shortcut
alias rst='shutdown -r now'

# shortcut for firefox
alias fire='firefox'

# shortcut for home directory
alias hm='cd ~'

# shortcut for password generator
